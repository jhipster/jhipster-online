    "eslint-config-prettier": "6.7.0",
    "eslint-loader": "3.0.3",
    "file-loader": "5.0.2",
    "friendly-errors-webpack-plugin": "1.7.0",
    "generator-jhipster": "6.10.1",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
